logLevel: debug
image:
  tag: $IMAGE_TAG
  pullPolicy: Never

  config:
    sources:
      - kind: okta
        domain: $OKTA_DOMAIN
        clientId: $OKTA_CLIENT_ID
        clientSecret: $OKTA_SECRET/clientSecret
        apiToken: $OKTA_SECRET/apiToken

engine:
  name: docker-desktop
  image:
    tag: $IMAGE_TAG
    pullPolicy: Never
  service:
    ports:
      - name: https
        port: 8443
        targetPort: 443
  logLevel: debug
