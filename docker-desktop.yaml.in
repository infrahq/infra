log-level: debug
image:
  tag: $IMAGE_TAG
  pullPolicy: Never

config:
  sources:
    - kind: okta
      domain: $OKTA_DOMAIN
      client-id: $OKTA_CLIENT_ID
      client-secret: $OKTA_SECRET/clientSecret
      okta:
        api-token: $OKTA_SECRET/apiToken

engine:
  name: docker-desktop
  image:
    tag: $IMAGE_TAG
    pullPolicy: Never
  service:
    ports:
      - name: https
        port: 8443
        targetPort: 443
  log-level: debug
