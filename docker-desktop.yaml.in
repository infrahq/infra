global:
  image:
    tag: $IMAGE_TAG
    pullPolicy: Never

server:
  additionalProviders:
    - name: okta
      url: $OKTA_DOMAIN
      clientID: $OKTA_CLIENT_ID
      clientSecret: $OKTA_CLIENT_SECRET

  additionalGroups:
    - name: Everyone
      provider: okta
      grants:
        - kind: cluster-role
          name: cluster-admin
          destinations:
            - kind: kubernetes

engine:
  config:
    name: docker-desktop
