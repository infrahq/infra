{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently 'npm run dev:ui' 'npm run dev:server'",
    "dev:ui": "next dev",
    "dev:server": "go run .. server --enable-ui --ui-proxy-url http://localhost:3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "standard": "standard --fix",
    "export": "rm -rf .next out && npm install --silent --no-progress && next build && next export"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "dayjs": "^1.11.0",
    "next": "^12.1.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^6.3.0",
    "react-table": "^7.7.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.26",
    "@types/react": "18.0.6",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "eslint-config-next": "^12.1.4",
    "postcss": "^8.4.12",
    "standard": "^16.0.4",
    "tailwindcss": "^3.0.23"
  },
  "standard": {
    "globals": [
      "fetch"
    ]
  }
}
