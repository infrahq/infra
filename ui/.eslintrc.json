{
  "extends": ["eslint:recommended", "next/core-web-vitals", "prettier"],
  "rules": {
    "@next/next/no-img-element": "off",
    "react-hooks/exhaustive-deps": "off"
  },
  "plugins": ["jest"],
  "globals": {
    "Promise": true,
    "Set": true,
    "jsonBody": true
  },
  "env": {
    "jest/globals": true,
    "es6": true
  }
}
