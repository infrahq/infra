{
  "files": [
    {
      "pattern": "infra_*.zip",
      "target": "generic-local/$GITHUB_REPOSITORY/"
    },
    {
      "pattern": "infra_*_amd64.deb",
      "target": "deb-local/$GITHUB_REPOSITORY/",
      "props": "deb.distribution=$DEB_DISTRIBUTION;deb.component=$DEB_COMPONENT,deb.architecture=amd64"
    },
    {
      "pattern": "infra_*_arm64.deb",
      "target": "deb-local/$GITHUB_REPOSITORY/",
      "props": "deb.distribution=$DEB_DISTRIBUTION;deb.component=$DEB_COMPONENT,deb.architecture=arm64"
    },
    {
      "pattern": "infra-*.rpm",
      "target": "rpm-local/$GITHUB_REPOSITORY/"
    },
    {
      "pattern": "infra-*.tgz",
      "target": "helm-local/$GITHUB_REPOSITORY/"
    },
    {
      "pattern", "infra_*/infra*",
      "target": "go-local/$GITHUB_REPOSITORY/"
    }
  ]
}
